{
  "name": "bitshares_astro_ui",
  "productName": "Bitshares Astro UI",
  "version": "0.0.2",
  "main": "app/background.js",
  "homepage": "https://github.com/bts-cm/astro-ui",
  "repository": "github:bts-cm/astro-ui",
  "author": "BeetEOS team <nftea.gallery@gmail.com>",
  "description": "Bitshares market UI",
  "license": "MIT",
  "build": {
    "appId": "bitshares_astro_ui",
    "productName": "Bitshares Astro UI",
    "artifactName": "BTSAstroUI.${ext}",
    "files": [
      "app/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "category": "Utility",
      "target": ["deb", "AppImage"],
      "mimeTypes": [],
      "desktop": {
        "exec": "BTSAstroUI %U"
      }
    }
  },
  "dependencies": {
    "@astrojs/node": "^8.3.2",
    "@astrojs/react": "^3.6.0",
    "@astrojs/tailwind": "^5.1.0",
    "@babel/runtime": "^7.25.0",
    "@exodus/bytebuffer": "git@github.com:BTS-CM/bytebuffer.js.git#393541bd29f988a2af6a7785b1ee4978881ed6bb",
    "@hookform/resolvers": "^3.9.0",
    "@nanostores/persistent": "^0.10.1",
    "@nanostores/query": "^0.3.3",
    "@nanostores/react": "^0.7.2",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@react-hook/throttle": "^2.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "assert": "^2.1.0",
    "astro": "^4.12.2",
    "bigi": "^1.4.2",
    "bignumber.js": "^9.1.2",
    "bitsharesjs": "https://github.com/bitshares/bitsharesjs#develop",
    "bs58": "^6.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^0.2.0",
    "create-hash": "^1.2.0",
    "create-hmac": "^1.1.7",
    "date-fns": "^3.3.1",
    "deep-equal": "^2.2.3",
    "ecurve": "^1.0.6",
    "electron-devtools-installer": "^3.2.0",
    "express": "^4.19.2",
    "fuse.js": "^7.0.0",
    "isomorphic-ws": "^5.0.0",
    "long": "^5.2.3",
    "nanostores": "^0.10.3",
    "react": "^18.3.1",
    "react-day-picker": "^9.0.4",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-i18next": "^15.0.0",
    "react-intersection-observer": "^9.13.0",
    "react-window": "^1.8.10",
    "safe-buffer": "^5.2.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss": "^3.4.7",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.0",
    "@soda/friendly-errors-webpack-plugin": "^1.8.1",
    "babel-loader": "^9.1.3",
    "electron": "^31.3.0",
    "electron-builder": "^24.13.3",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "eraseChainData": "rm -rf ./src/data/bitshares/* && rm -rf ./src/data/bitshares_testnet/*",
    "erasePools": "rm -rf ./src/data/bitshares/allPools.json && rm -rf ./src/data/bitshares/minPools.json && rm -rf ./src/data/bitshares_testnet/allPools.json && rm -rf ./src/data/bitshares_testnet/minPools.json",
    "eraseAssets": "rm -rf ./src/data/bitshares/assets/* && rm -rf ./src/data/bitshares_testnet/assets/*",
    "eraseDynamicData": "rm -rf ./src/data/bitshares/dynamic/* && rm -rf ./src/data/bitshares_testnet/dynamic/*",
    "eraseIssuers": "rm -rf ./src/data/bitshares/issuers/* && rm -rf ./src/data/bitshares_testnet/issuers/*",
    "eraseBitassetData": "rm -rf ./src/data/bitshares/bitassets/* && rm -rf ./src/data/bitshares_testnet/bitassets/*",
    "eraseOffers": "rm -rf ./src/data/bitshares/offers/* && rm -rf ./src/data/bitshares_testnet/offers/*",
    "eraseDeals": "rm -rf ./src/data/bitshares/deals/* && rm -rf ./src/data/bitshares_testnet/deals/*",
    "eraseFees": "rm -rf ./src/data/bitshares/fees/* && rm -rf ./src/data/bitshares_testnet/fees/*",
    "refreshData": "rm -rf ./src/data/bitshares/* && rm -rf ./src/data/bitshares_testnet/* && bun run fetchPools && bun run fetchFees && bun run fetchAssets && bun run fetchDynamicData && bun run fetchIssuers && bun run fetchBitassetData && bun run fetchOffers",
    "fetchPools": "bun run ./src/data/fetchPools.js",
    "fetchAssets": "bun run ./src/data/fetchAssets.js",
    "fetchDynamicData": "bun run ./src/data/fetchDynamicData.js",
    "fetchIssuers": "bun run ./src/data/fetchAssetIssuers.js",
    "fetchBitassetData": "bun run ./src/data/fetchBitassetData.js",
    "fetchOffers": "bun run ./src/data/fetchCreditOffers.js",
    "fetchDeals": "bun run ./src/data/fetchCreditDeals.js",
    "fetchFees": "bun run ./src/data/fetchFees.js",
    "emptySpace": "",
    "dev": "astro dev",
    "build:astro": "astro build",
    "start": "node build/start.js",
    "compile": "webpack --config=build/webpack.main.config.js --config=build/webpack.preload.config.js --env=production",
    "dist:macos-latest": "bun run compile && electron-builder -m --publish never",
    "dist:windows-latest": "bun run compile && electron-builder -w --publish never",
    "dist:ubuntu-latest": "bun run compile && electron-builder -l --publish never"
  }
}
